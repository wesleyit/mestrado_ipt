	keywords = {{IoT}, 5G slicing, distributed cloud, edge cloud, orchestration},
}

@inproceedings{sritriratanarak_intelligent_2023,
	location = {New York, {NY}, {USA}},
	title = {Intelligent Autonomous Robots: Cooperative Sharing for Energy Management in Industrial {IoT}},
	isbn = {9798400708312},
	url = {https://doi.org/10.1145/3625343.3625347},
	doi = {10.1145/3625343.3625347},
	series = {{AIMLR} '23},
	abstract = {Industrial {IoT} offers several challenges compared to commodity {IoT}: safety and fault tolerance requirements, resource management, constrained environments and bandwidth, etc. However, as the deployment of industrial {IoT} is, more and more, built on (semi-)autonomous edge devices such as mobile robots, new opportunities to improve operational features emerge. We explore embedding autonomous mobile robots, responsible for effecting work in an industrial environment, within the industrial {IoT} resource management sub-system. This approach brings resource management closer to the edge, lending itself to more distributed intelligence, compared to cloud/fog-only solutions. Specifically, we demonstrate that intelligent autonomous mobile robots can cooperate, sharing energy with one another to minimize: (1) hard failures; situations where robots are left inert, after their energy is completely depleted, resulting in safety concerns and impeding other robots’ actions; and (2) soft failures: where robots are idle, performing no useful work, as a consequence of having to wait for the availability of charging stations. We evaluate this hypothesis using simulation techniques. Results suggest cooperative energy management is effective in increasing productivity and reducing the number of inert robots that require intervention to recover from hard failures, when the strategy is applied only to robots that have become inert. Interestingly, applying energy sharing indiscriminately (to both inert and idle robots) results in negative consequences. Thus, our results suggest cooperative energy management is a promising technique to improve industrial {IoT}, but must be applied strategically after evaluating operational properties, through either empirical or simulation methods.},
	booktitle = {Proceedings of the 2023 Asia Conference on Artificial Intelligence, Machine Learning and Robotics},
	publisher = {Association for Computing Machinery},
	author = {Sritriratanarak, Warisa and Garcia, Paulo},
	date = {2023},
