	journaltitle = {{IEEE} Internet of Things Journal},
	author = {Suhail, Sabah and Hussain, Rasheed and Khan, Abid and Hong, Choong Seon},
	date = {2021-01},
	keywords = {Blockchain, Computers, Cryptography, Digital signatures, distributed ledger technology ({DLT}), hash-based signature ({HBS}), Internet of Things, Internet of Things ({IoT}), Internet-of-Things ({IoT}) security, postquantum cryptography ({PQC}), public-key cryptography, quantum computing, Quantum computing, Standardization},
