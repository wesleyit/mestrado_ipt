	author = {Fernández-Caramés, Tiago M.},
	urldate = {2023-10-17},
	date = {2020-07},
	note = {Conference Name: {IEEE} Internet of Things Journal},
	keywords = {Computers, Internet of Things, Internet of Things ({IoT}), Quantum computing, {IoT} security, Elliptic curve cryptography, post-quantum, quantum-resistant, quantum-safe},
	file = {IEEE Xplore Abstract Record:/home/wrodrigues/Zotero/storage/Y2KB9S6Q/8932459.html:text/html;IEEE Xplore Full Text PDF:/home/wrodrigues/Zotero/storage/PTVSA3P4/Fernández-Caramés - 2020 - From Pre-Quantum to Post-Quantum IoT Security A S.pdf:application/pdf},
