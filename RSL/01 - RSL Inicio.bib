	note = {Conference Name: {IEEE} Internet of Things Journal},
	keywords = {Internet of Things, Public key, Encryption, Security, Hardware, Lattices, lattice-based cryptography, Software, Internet of Things ({IoT}) security, Hardware implementation, ring learning with errors (Ring-{LWE}), software implementation},
	file = {IEEE Xplore Full Text PDF:/home/wrodrigues/Zotero/storage/SA6M3NSA/Xu et al. - 2022 - Ring-ExpLWE A High-Performance and Lightweight Po.pdf:application/pdf},
